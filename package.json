{
  "name": "lazyvax-api",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "DEV_ENV=true nodemon -L index.js",
    "build": "babel src --out-dir build --copy-files --extensions '.ts,.js'",
    "debug": "nodemon --inspect=0.0.0.0:9229 ./index.js",
    "test": "jest --passWithNoTests --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:dev": "docker compose exec nodejs yarn test",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "_moduleAliases": {
    "@": "src",
    "@common": "src/common",
    "@controllers": "src/controllers",
    "@dao": "src/dao",
    "@middlewares": "src/middlewares",
    "@models": "src/models",
    "@routes": "src/routes",
    "@services": "src/services",
    "@validators": "src/validators"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "npm test",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**",
      "**/data/**"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@babel/cli": "^7.25.7",
    "@babel/register": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "@google-cloud/text-to-speech": "^5.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "debug": "~4.3.7",
    "dotenv": "^16.4.5",
    "express": "~4.21.1",
    "express-jwt": "^8.4.1",
    "express-session": "^1.18.1",
    "express-validator": "^6.14.3",
    "google-auth-library": "9.14.2",
    "googleapis": "^144.0.0",
    "ioredis": "^5.4.1",
    "jwks-rsa": "^3.1.0",
    "module-alias": "^2.2.3",
    "mongodb": "^5.0.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.6",
    "nodemon": "^2.0.20",
    "openai": "^4.67.3",
    "prettier": "^2.8.4",
    "socket.io": "^4.8.0",
    "ts-node": "^10.9.2",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-proposal-do-expressions": "^7.25.8",
    "@babel/plugin-proposal-export-default-from": "^7.25.8",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.25.8",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-pipeline-operator": "^7.25.7",
    "@babel/plugin-proposal-throw-expressions": "^7.25.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-optional-chaining": "^7.25.8",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/express": "^4.17.17",
    "@types/node": "^22.7.5",
    "@types/supertest": "^2.0.12",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "body-parser": "^1.20.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-express": "^1.12.0",
    "jest-runner": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^13.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "validator": "^13.12.0"
  }
}
