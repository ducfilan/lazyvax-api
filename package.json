{
  "name": "lazyvax-api",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "DEV_ENV=true nodemon -L index.js",
    "build": "babel src --out-dir build --copy-files --extensions '.ts,.js'",
    "debug": "nodemon --inspect=0.0.0.0:9229 ./index.js",
    "test": "jest --passWithNoTests --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:dev": "docker compose exec nodejs yarn test",
    "test:watch": "jest --passWithNoTests --watch"
  },
  "_moduleAliases": {
    "@": "build",
    "@common": "build/common",
    "@controllers": "build/controllers",
    "@dao": "build/dao",
    "@middlewares": "build/middlewares",
    "@models": "build/models",
    "@routes": "build/routes",
    "@services": "build/services",
    "@validators": "build/validators"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "eslint --fix",
        "npm test",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**",
      "**/data/**"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.303.0",
    "@aws-sdk/s3-request-presigner": "^3.303.0",
    "@babel/cli": "^7.20.7",
    "@google-cloud/text-to-speech": "^4.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "~4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.3",
    "ioredis": "^5.3.1",
    "jest-runner": "^29.4.3",
    "jwks-rsa": "^3.0.1",
    "module-alias": "^2.2.2",
    "mongodb": "^5.0.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.6",
    "nodemon": "^2.0.20",
    "openai": "^3.2.1",
    "prettier": "^2.8.4",
    "socket.io": "^4.6.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.20.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.13",
    "@jest/globals": "^29.4.3",
    "@jest/types": "^29.4.3",
    "@types/express": "^4.17.17",
    "@types/express-handlebars": "^6.0.0",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.4.3",
    "body-parser": "^1.20.1",
    "google-auth-library": "^8.7.0",
    "jest": "^29.4.3",
    "jest-cli": "^29.4.3",
    "jest-express": "^1.12.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "validator": "^13.9.0"
  }
}
